import turtle 

a = 20
for i in range(10):
	turtle.forward(a)
	turtle.right(90)
	turtle.forward(a)
	turtle.right(90)
	turtle.forward(a)
	turtle.right(90)
	turtle.forward(a)
	turtle.left(90)
	turtle.penup()
	turtle.forward(20)
	turtle.right(90)
	turtle.forward(20)
	turtle.right(90)
	turtle.pendown()
	a = a + 40
