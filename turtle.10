import turtle 

def circle(R):
	for i in range(500):
		turtle.forward(2*3.14*R/500)
		turtle.left(360/500)

turtle.shape('turtle')
turtle.speed(5)
circle(100)
turtle.left(180)
circle(100)
turtle.left(60)
circle(100)
turtle.left(180)
circle(100)
turtle.left(60)
